# FreeType 2 src/cid Jamfile (c) 2001 David Turner
#

SubDir  FT2_TOP $(FT2_SRC_DIR) cid ;

{
  local  _sources ;

  if $(FT2_MULTI)
  {
    _sources = cidobjs cidload cidgload cidriver cidparse ;
  }
  else
  {
    _sources = type1cid ;
  }

  Library  $(FT2_LIB) : $(_sources).c ;
}

# end of src/cid Jamfile

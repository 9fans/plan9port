# FreeType 2 src/type42 Jamfile (c) 2002 David Turner
#

SubDir  FT2_TOP $(FT2_SRC_DIR) type42 ;

{
  local  _sources ;

  if $(FT2_MULTI)
  {
    _sources = t42objs t42parse t42drivr ;
  }
  else
  {
    _sources = type42 ;
  }

  Library  $(FT2_LIB) : $(_sources).c ;
}

# end of src/type42 Jamfile

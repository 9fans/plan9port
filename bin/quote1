#!/usr/local/plan9/bin/rc

. 9.rc

fn text {
	if(~ $winid [0-9]*)
		9p read acme/$winid/body
	if not if(~ $text9term unix!*)
		dial -e $text9term </dev/null
	if not
		status=''
}

PROMPT='[^ 	]*[%;$#][ 	]+'

fn cmds {
	text | sed -n 's/^'$PROMPT'([^"])/	\1/p'
}

switch($#*) {
case 0
	cmds | tail -1
case *
	cmds | grep -n '^	'^$"* | tail -r |
		sort -u +1 | sort -n |
		sed 's/^[0-9]+: //'
}
